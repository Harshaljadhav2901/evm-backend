{
  "name": "hemantrasane/api",
  "description": "RESTful API for EVM Simulator",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "slim/slim": "^4.0",
    "slim/psr7": "^1.0",
    "nyholm/psr7": "^1.5",
    "nyholm/psr7-server": "^1.0",
    "php-di/php-di": "^6.0",
    "slim/http": "^1.0",
    "monolog/monolog": "^2.0",
    "vlucas/phpdotenv": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "process-timeout": 0
  },
  "scripts": {
    "start": "php -S localhost:8000 -t public public/index.php"
  }
}

